<h1>Grupos</h1>

<table>

<% for group in @groups %>
  <tr>
      <td class="date"><%= group.users.size %> personas</td>
      <td class="title"><%= link_to  group.name, group %></td>
      <td class="description"><p><%= group.description %></p></td>
      <td class="links"></td>
  </tr>
<% end %>
</table>


